{% extends 'AlbatrossUserBundle:Default:layout_v2.html.twig' %}

{% block title %}
	{{'Project: ' ~ project.name}}
{% endblock %}

{% block breadcrumb %}
	<li><a href="{{ path('project_homepage') }}">Project View</a><span class="divider"><i class="icon-angle-right"></i></span></li>
	<li class="active"> {{project.name}}</li>
{% endblock %}

{% block pageHeading %}
	{{project.name}}
{% endblock %}	

{% block body %}

<!--BODY STARTS-->
		
<div class="row-fluid">
	<div class="span-12">
		<ul class="successUl" style="display:none;">
			<li>{{successMsg}}</li>
		</ul>
		
		<div class="tab-widget">
			<ul class="nav nav-tabs" id="myTab1">
				{% set counter = 1 %}
				{% for wave in waveArr %}
					<li {% if counter==1 %}class="active"{% endif %}><a href="#tab-wave{{counter}}">Wave {{counter}}</a></li>
					{% set counter = counter+1 %}
				{% endfor %}
				<li><a href="#tab-new-wave">New Wave +</a></li>
			</ul>
			<div class="tab-content">
				<!--Code for fetching and displaying waves starts here-->
				{% set counter = 1 %}
				{% for wave in waveArr %}
				<div class="tab-pane {% if counter==1 %}active{% endif %}" id="tab-wave{{counter}}">
					<div class="user_list">
						<div class="">
							<div class="thumbnail pull-left">
								<img alt="300x200" data-src="holder.js/300x200" style="width: 200px; height: 200px;" src="data:image/png;base64,">
							</div>
							<div class="pull-left span6">
								<div class="wave-content-list">
									<label class="pull-left"><strong>Report KA:</strong></label>
									<label>{{wave.user.fullname}}</label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><strong>Global PM:</strong></label>
									<label>{{wave.projectManager.fullname}}</label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><strong>Client:</strong></label>
									<label>{{project.customclient.name}}</label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><strong>Project:</strong></label>
									<label>{{project.name}}</label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><strong>FW Period:</strong></label>
									<label><em>{{taskArr[wave.id][0].fwstartdate}}</em> to <em>{{taskArr[wave.id][0].fwenddate}}</em></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><strong>ACE Project:</strong></label>
									<label>{{wave.name}} [ <em># {{taskArr[wave.id][0].projectNumber}}</em> ]</label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><strong>AOL Questionnaire:</strong></label>
									<label>{% if wave['campaign'][0] is defined %} {{wave['campaign'][0]['questionnaire']['name']}} (<em>Campaign: {{wave['campaign'][0]['name']}}</em>) {% else %} &nbsp; {% endif%}</label>
								</div>
								<div class="wave-content-list">
									<a href="#">Edit Wave</a>
								</div>
							</div>
							<div class="pull-right">
								<button class="btn btn-primary" type="button"><i class="icon-bar-chart"></i> GANTT CHART</button>
								<div class="btn-group">
									<button class="btn btn-success" style="width:137px; text-align:left;">Submit</button>
									<button data-toggle="dropdown" class="btn btn-success dropdown-toggle"><span class="icon-ok"></span></button>
									<ul class="dropdown-menu submit-dropdown">
										<li><a href="javascript:;" onclick="showMiniForm('meeting-minutes-frm{{wave.id}}');"><i class="icon-plus-sign gray"></i>Meeting Minutes</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('brand-material-frm{{wave.id}}');"><i class="icon-plus-sign gray"></i>Brand Material</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('kick-off-meetings-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>Kick-Off Meeting</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('questionnaire-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>Questionnaire</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('pos-file-upload-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>POS List</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('spe-brief-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>SPE Brief</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('upload-iof-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>IOF</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('fw-recap-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>FW Recap</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('dic-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>DIC</a></li>
										<li><a href="javascript:;" onclick="showMiniForm('report-frm{{wave.id}}');"><i class="icon-ok-sign green"></i>Report</a></li>
									</ul>
								</div>
							</div>
							<div class="clearfix"></div>
						</div>
						<div class="span8">
							<div class="primary-head">
								<br />
								<h3 class="page-header">Wave 1</h3>
							</div>
							<table class="stat-table table table-stats table-striped table-sorted table-bordered">
								<tbody>
									<tr>
										<th>Operations</th>
										<th>Step</th>
										<th>Number of Surveys</th>
										<th>FW Start</th>
										<th>FW End</th>
										<th>% Assigned</th>
										<th>% FW Done</th>
										<th>% Editing Done</th>
										<th>Report Delivery</th>
									</tr>
									{% if operation[wave.id] is defined %}
										{% for bu,info in operation[wave.id] %}
										<tr>
											<td title="{{bu}}">{{bu}}</td>
											{% if info['ace'] != '' %}
												{% for pname,aceinfo in info['ace'] %}
												<td>
													{{aceinfo['step']}}<br/>
												</td>
												<td>
													{{aceinfo['scope']}}<br/>
												</td>
												<td>
													{{aceinfo['fwstartdate']}}<br/>
												</td>
												<td>
													{{aceinfo['fwenddate']}}<br/>
												</td>
												{% endfor %}
											{% else %}
												<td class="empty_value">empty</td>
												<td class="empty_value">empty</td>
												<td class="empty_value">empty</td>
												<td class="empty_value">empty</td>
											{% endif %}
											
											{% if info['aol'] != '' %}
												<td>{{info['aol']['assignPercent']}}
												</td>
												<td>{{info['aol']['fwdonePercent']}}
												</td>
												<td>{{info['aol']['validationPercent']}}
												</td>
											{% else %}
												<td class="empty_value">empty</td>
												<td class="empty_value">empty</td>
												<td class="empty_value">empty</td>
											{% endif %}
											{% if info['ace'] != '' %}
												{% for pname,aceinfo in info['ace'] %}
												<td>{{aceinfo['reportduedate']}}</td>
												{% endfor %}
											{% else %}
												<td class="empty_value">empty</td>
											{% endif %}
										</tr>
										{% endfor %}
									{% else %}
									<tr>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
										<td class="empty_value">empty</td>
									</tr>
									{% endif %}
								</tbody>
							</table>
							
							<!--Other submit forms starts-->
							
							<!--Meeting minutes form starts-->
							<form name="meeting_minutes_frm{{wave.id}}" id="meeting_minutes_frm{{wave.id}}" action="" method="post" onSubmit="return chkProjectForms('meetingMinutes','meeting_minutes_frm{{wave.id}}',{{wave.id}});">
							<input type="hidden" name="action" value="meetingMinutes" />
							<input type="hidden" name="wave_id" value="{{wave.id}}" />
							<div class="span12 mini-form meeting-minutes-frm{{wave.id}}" style="display:none;">
								<h3 class="page-header">Meeting minutes</h3>
								<div class="wave-content-list">
									<label class="pull-left">Project Name:</label>
									<label><strong>{{project.name}}</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Wave Name:</label>
									<label><strong>{{wave.name}}</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Submit Time:</label>
									<label><strong>
										{% if formArr[wave.id]['mm']['submittime'] is defined %}
											{{formArr[wave.id]['mm']['submittime']}}
										{% else %}
											&nbsp;
										{% endif %}
									</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Submit User:</label>
									<label><strong>
										{% if formArr[wave.id]['mm']['user']['fullname'] is defined %}
											{{formArr[wave.id]['mm']['user']['fullname']}}
										{% else %}
											&nbsp;
										{% endif %}
									</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Brand:</label>
									<input type="text" name="mm_brand" id="mm_brand{{wave.id}}" required="" value="{% if formArr[wave.id]['mm']['mmBrand'] is defined %} {{formArr[wave.id]['mm']['mmBrand']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Date:</label>
									<input type="text" name="mm_date" id="mm_date{{wave.id}}" required="" value="{% if formArr[wave.id]['mm']['mmDate'] is defined %} {{formArr[wave.id]['mm']['mmDate']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Attendies:</label>
									<input type="text" name="name" id="name" placeholder="Name" required="" />
									<input type="text" name="position" id="position" placeholder="Position" style="width:160px;" required="" />
									<span>Albatross Attendees</span> <input type="checkbox" name="" id="" style="vertical-align:middle;" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Address:</label>
									<input type="text" name="mm_address" id="mm_address{{wave.id}}" required="" value="{% if formArr[wave.id]['mm']['mmAddress'] is defined %} {{formArr[wave.id]['mm']['mmAddress']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Purpose:</label>
									<input type="text" name="mm_purpose" id="mm_purpose{{wave.id}}" required="" value="{% if formArr[wave.id]['mm']['mmPurpose'] is defined %} {{formArr[wave.id]['mm']['mmPurpose']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Next Step:</label>
									<input type="text" name="mm_nextstep" id="mm_nextstep{{wave.id}}" required="" value="{% if formArr[wave.id]['mm']['mmNextstep'] is defined %} {{formArr[wave.id]['mm']['mmNextstep']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Agenda of the meeting:</label>
									<textarea name="mm_agenda_of_the_meeting" id="mm_agenda_of_the_meeting{{wave.id}}" required="">{% if formArr[wave.id]['mm']['mmAgendaOfTheMeeting'] is defined %} {{formArr[wave.id]['mm']['mmAgendaOfTheMeeting']}} {% endif %}</textarea>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Clients Feedback:</label>
									<textarea name="mm_clients_feedback" id="mm_clients_feedback{{wave.id}}" required="">{% if formArr[wave.id]['mm']['mmClientsFeedback'] is defined %} {{formArr[wave.id]['mm']['mmClientsFeedback']}} {% endif %}</textarea>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Comments:</label>
									<textarea name="mm_comments" id="mm_comments{{wave.id}}" required="">{% if formArr[wave.id]['mm']['mmComments'] is defined %} {{formArr[wave.id]['mm']['mmComments']}} {% endif %}</textarea>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">No file to upload:</label>
									<input type="checkbox" name="" id="" style="vertical-align:middle;" />
								</div>
								<div class="wave-content-list">
									<button class="btn btn-blue"><span class="icon-ok"></span> &nbsp; Submit</button> &nbsp; 
									<button class="btn">Cancel</button>
								</div>
							</div>
							</form>
							<!--Meeting minutes form ends-->
								
							<!--Brand material form starts-->
							<form name="brand_frm{{wave.id}}" id="brand_frm{{wave.id}}" action="" method="post"  enctype="multipart/form-data" onsubmit="chkProjectForms('brand','brand_frm{{wave.id}}',{{wave.id}});">
							<input type="hidden" name="action" value="brand" />
							<input type="hidden" name="wave_id" value="{{wave.id}}" />
							<div class="span10 mini-form brand-material-frm{{wave.id}}" style="display:none;">
								<h3 class="page-header">Brand Material</h3>
								<div class="wave-content-list">
									<label class="pull-left">Project Name:</label>
									<label><strong>{{project.name}}</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Wave Name:</label>
									<label><strong>{{wave.name}}</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Submit Time:</label>
									<label><strong>
										{% if formArr[wave.id]['brand']['submittime'] is defined %}
											{{formArr[wave.id]['brand']['submittime']}}
										{% else %}
											&nbsp;
										{% endif %}
									</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Submit User:</label>
									<label><strong>
										{% if formArr[wave.id]['brand']['user']['fullname'] is defined %}
											{{formArr[wave.id]['brand']['user']['fullname']}}
										{% else %}
											&nbsp;
										{% endif %}
									</strong></label>
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>File:</label>
									<input type="file" name="fileBrand" id="fileBrand{{wave.id}}" />
									{% if formArr[wave.id]['brand']['path'] is defined %}
										{% if formArr[wave.id]['brand']['path']!='' %}
											<br/><a href="{{FILE_SITE_URL ~ formArr[wave.id]['brand']['path']}}" target="_blank" style="margin-left:150px;">{{formArr[wave.id]['brand']['path']}}</a>
										{% endif %}
									{% endif %}
								</div>
								<div class="wave-content-list">
									<label class="pull-left"><span class="error">*</span>Material Name:</label>
									<select name="material_name" id="material_name{{wave.id}}" required="">
										<option value="">Please select</option>
										{% for mid,mname in material %}
											{% set sel = '' %}
											{% if formArr[wave.id]['brand']['materialName'] is defined %}
												{% if formArr[wave.id]['brand']['materialName'] == mid %}
													{% set sel = 'selected="selected"' %}
												{% endif %}
											{% endif %}	
											<option value="{{mid}}" {{sel}}>{{mname}}</option>
										{% endfor %}
									</select>
								</div>
								<div class="wave-content-list">
									<label class="pull-left">No file to upload:</label>
									<input type="checkbox" name="mNoFileUpload" id="mNoFileUpload{{wave.id}}" style="vertical-align:middle;" />
								</div>
								<div class="wave-content-list">
									<button class="btn btn-blue"><span class="icon-ok"></span> &nbsp; Submit</button> &nbsp; 
									<button type="reset" class="btn">Cancel</button>
								</div>
							</div>
							</form>
							<!--Brand material form ends-->
							
							<!--Kick off meetings form starts-->
							<form name="kick_off_frm{{wave.id}}" id="kick_off_frm{{wave.id}}" action="" method="post">
							<input type="hidden" name="action" value="kick_off_meeting" />
							<input type="hidden" name="wave_id" value="{{wave.id}}" />
							<div class="span12 mini-form large_label kick-off-meetings-frm{{wave.id}}" style="display:none;">
								<h3 class="page-header">Kick Off Meeting Recap</h3>
								<div class="wave-content-list">
									<label class="pull-left">PM Attendee:</label>
									<input type="text" name="pm_attendee" id="pm_attendee" value="{% if formArr[wave.id]['meeting']['pmAttendee'] is defined %} {{formArr[wave.id]['meeting']['pmAttendee']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">OP Attendee:</label>
									<input type="text" name="op_attendee" id="op_attendee" value="{% if formArr[wave.id]['meeting']['opAttendee'] is defined %} {{formArr[wave.id]['meeting']['opAttendee']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">VA Attendee:</label>
									<input type="text" name="va_attendee" id="va_attendee" value="{% if formArr[wave.id]['meeting']['vaAttendee'] is defined %} {{formArr[wave.id]['meeting']['vaAttendee']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">QC Attendee:</label>
									<input type="text" name="qc_attendee" id="qc_attendee" value="{% if formArr[wave.id]['meeting']['qcAttendee'] is defined %} {{formArr[wave.id]['meeting']['qcAttendee']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Report Attendee:</label>
									<input type="text" name="report_attendee" id="report_attendee" value="{% if formArr[wave.id]['meeting']['reportAttendee'] is defined %} {{formArr[wave.id]['meeting']['reportAttendee']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Brand Positioning, Products categories:</label>
									<input class="large_input" type="text" name="text_1" id="text_1" value="{% if formArr[wave.id]['meeting']['text1'] is defined %} {{formArr[wave.id]['meeting']['text1']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">What are the objectives of this store Evalution Program?:</label>
									<input class="large_input" type="text" name="text_2" id="text_2" value="{% if formArr[wave.id]['meeting']['text2'] is defined %} {{formArr[wave.id]['meeting']['text2']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Key points to pay attention for the report:</label>
									<input class="large_input" type="text" name="text_3" id="text_3" value="{% if formArr[wave.id]['meeting']['text3'] is defined %} {{formArr[wave.id]['meeting']['text3']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Timeline:</label>
									<input class="large_input" type="text" name="text_4" id="text_4" value="{% if formArr[wave.id]['meeting']['text4'] is defined %} {{formArr[wave.id]['meeting']['text4']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Scenario/key points for OP surveys screening:</label>
									<input class="large_input" type="text" name="text_5" id="text_5" value="{% if formArr[wave.id]['meeting']['text5'] is defined %} {{formArr[wave.id]['meeting']['text5']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Key points for SPE profile and recruitment:</label>
									<input class="large_input" type="text" name="text_6" id="text_6" value="{% if formArr[wave.id]['meeting']['text6'] is defined %} {{formArr[wave.id]['meeting']['text6']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Training methods:</label>
									<input class="large_input" type="text" name="text_7" id="text_7" value="{% if formArr[wave.id]['meeting']['text7'] is defined %} {{formArr[wave.id]['meeting']['text7']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Payrate and flexibility/Bonus:</label>
									<input class="large_input" type="text" name="text_8" id="text_8" value="{% if formArr[wave.id]['meeting']['text8'] is defined %} {{formArr[wave.id]['meeting']['text8']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Key points for the SPE Briefing:</label>
									<input class="large_input" type="text" name="text_9" id="text_9" value="{% if formArr[wave.id]['meeting']['text9'] is defined %} {{formArr[wave.id]['meeting']['text9']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Debrief of the pilot visit/Go through the Questionnaire:</label>
									<input class="large_input" type="text" name="text_10" id="text_10" value="{% if formArr[wave.id]['meeting']['text10'] is defined %} {{formArr[wave.id]['meeting']['text10']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Key points for editing/QC:</label>
									<input class="large_input" type="text" name="text_11" id="text_11" value="{% if formArr[wave.id]['meeting']['text11'] is defined %} {{formArr[wave.id]['meeting']['text11']}} {% endif %}" />
								</div>
								<div class="wave-content-list">
									<label class="pull-left">Specific requirements: misfires records for a targeted program, proof of visit...:</label>
									<input class="large_input" type="text" name="text_12" id="text_12" value="{% if formArr[wave.id]['meeting']['text12'] is defined %} {{formArr[wave.id]['meeting']['text12']}} {% endif %}" />
								</div>
								
								<div class="wave-content-list">
									<button class="btn btn-blue"><span class="icon-ok"></span> &nbsp; Submit</button> &nbsp; 
									<button class="btn">Cancel</button>
								</div>
							</div>
							</form>
							<!--Kick off meetings form ends-->
							
							<!--Questionnaire form starts-->
							<form name="questionnaire_frm{{wave.id}}" id="questionnaire_frm{{wave.id}}" action="" method="post" onsubmit="chkProjectForms('questionnaire','questionnaire_frm{{wave.id}}',{{wave.id}});" enctype="multipart/form-data" >
							<input type="hidden" name="action" value="questionnaire" />
							<input type="hidden" name="wave_id" value="{{wave.id}}" />
							<input type="hidden" name="wave_name" value="{{wave.name}}" />
