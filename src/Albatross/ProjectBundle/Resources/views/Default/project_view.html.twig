                        <input type="text" name="totalnum" id="totalnum" placeholder="Total Number" />
                    </div>
                    <div class="wave-content-list">
                        <label class="pull-left">Report KA:</label>
                        <select name="user_id" id="user_id">
                            <option value="">Select</option>
                            {% for uarr in userArr %}
                                <option value="{{uarr['id']}}">{{uarr['fullname']}}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="wave-content-list">
                        <label class="pull-left">Project Manager:</label>
                        <select name="project_manager_id" id="project_manager_id">
                            <option value="">Select</option>
                            {% for uarr in userArr %}
                                <option value="{{uarr['id']}}">{{uarr['fullname']}}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="wave-content-list">
                        <label class="pull-left">ACE Project:</label>
                        <select name="ace_project" id="ace_project">
                            <option value="">Select</option>
                            {% for aarr in aceArr %}
                                <option value="{{aarr['id']}}">{{aarr['name']}}</option>
                            {% endfor %}
                        </select>
                    </div>
                    <div class="wave-content-list">
                        <label class="pull-left" style="float:left;">AOL Survey:</label>
                        <select name="wave_questionnaire_id" id="wave_questionnaire_id" onchange="populateCampaign(this.value, 'wave_campaign_id', 'quesLoadingIcon');" style="float:left;">
                            <option value="">Select Questionnaire</option>
                            {% for qarr in quesArr %}
                                <option value="{{qarr['id']}}">{{qarr['name']}}</option>
                            {% endfor %}
                        </select> &nbsp;&nbsp; 
                        <select name="wave_campaign_id" id="wave_campaign_id" style="float:left; margin-left:10px;">
                            <option value="">Select Questionnaire first</option>
                        </select> &nbsp;&nbsp; 
                        <img src="{{FILE_SITE_URL}}web/images/loading.gif" id="quesLoadingIcon" style="float:left;margin:-2px 0 0 15px; display:none;" />
                    </div>

                    <div class="wave-content-list">
                        <label class="pull-left"></label>
                        <button class="btn btn-blue"><span class="icon-ok"></span> &nbsp; Submit</button> &nbsp; 
                        <button class="btn">Cancel</button>
                    </div>
