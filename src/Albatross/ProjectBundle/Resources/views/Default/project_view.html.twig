<script>
            $(function() {
            $('textarea.chat-inputbox').tinymce({
            script_url : 'js/tiny_mce/tiny_mce.js',
                    theme : "simple",
                    width : 350
            });        {% if successMsg != '' %}
                                $('.successUl').show('slow');        {% endif %}

                                                    setTimeout(function(){$('.successUl').hide('slow'); }, 5000);
                                                    });
                                            function showMiniForm(id)
                                                    {
                                                    $('.mini-form').hide('fast');
                                                            $('.' + id).show('slow');
                                                            $('body,html').animate({
                                                    scrollTop: 850
                                                    }, 500);
                                                            return false;
                                                            }

                                            function populateCampaign(quesId, waveCampSelId, quesLoadingId)
                                                    {
                                                    document.getElementById(quesLoadingId).style.display = 'block';
                                                            var result = $.ajax({
                                                            type: "POST",
                                                                    url: "{{path('populate_campaign')}}",
                                                                    data: 'id=' + quesId,
                                                                    async: false
                                                            }).responseText;
                                                            document.getElementById(waveCampSelId).innerHTML = result;
                                                            document.getElementById(quesLoadingId).style.display = 'none';
                                                            }

                                            function chkProjectForms(type, frmId, waveId)
                                                    {
                                                    var chk = 1;
                                                            if (type == 'meetingMinutes')
                                                    {
                                                    var mm_brand = document.getElementById('mm_brand' + waveId).value;
                                                            var mm_date = document.getElementById('mm_date' + waveId).value;
                                                            var mm_address = document.getElementById('mm_address' + waveId).value;
                                                            var mm_purpose = document.getElementById('mm_purpose' + waveId).value;
                                                            var mm_nextstep = document.getElementById('mm_nextstep' + waveId).value;
                                                            var mm_agenda_of_the_meeting = document.getElementById('mm_agenda_of_the_meeting' + waveId).value;
                                                            var mm_clients_feedback = document.getElementById('mm_clients_feedback' + waveId).value;
                                                            var mm_comments = document.getElementById('mm_comments' + waveId).value;
                                                            if (mm_brand == '' || mm_date == '' || mm_address == '' || mm_purpose == '' || mm_nextstep == '' || mm_agenda_of_the_meeting == '' || mm_clients_feedback == '' || mm_comments == '')
                                                    {
                                                    chk = 0;
                                                    }
                                                    }
                                                    else if (type == 'brand')
                                                    {
                                                    var fileBrand = document.getElementById('fileBrand' + waveId).value;
                                                            var material_name = document.getElementById('material_name' + waveId).value;
                                                            var mNoFileUploadId = document.getElementById('mNoFileUpload' + waveId);
                                                            if (material_name == '')
                                                    {
                                                    chk = 0;
                                                    }

                                                    if (fileBrand == '' && mNoFileUploadId.checked == false)
                                                    {
                                                    alert('Please either select file to upload or check "No file to upload".');
                                                            return false;
                                                    }
                                                    }
                                                    else if (type == "questionnaire")
                                                    {
                                                    var path = document.getElementById('path' + waveId).value;
                                                            var file1_label = document.getElementById('question_file1_label' + waveId).value;
                                                            var quesNoFileUploadId = document.getElementById('quesNoFileUpload' + waveId);
                                                            if (path == '' && quesNoFileUploadId.checked == false)
                                                    {
                                                    alert('Please either select file to upload or check "No file to upload".');
                                                            return false;
                                                    }
                                                    }
                                                    else if (type == 'pos_file')
                                                    {
                                                    var filePos = document.getElementById('pos_list_file' + waveId).value;
                                                            if (filePos == '')
                                                    {
                                                    chk = 0;
                                                    }
                                                    }
                                                    else if (type == 'brief')
                                                    {
                                                    var spePath = document.getElementById('spePath' + waveId).value;
                                                            var not_file_upload_spe = document.getElementById('not_file_upload_spe' + waveId);
                                                            if (material_name == '')
                                                    {
                                                    chk = 0;
                                                    }

                                                    if (spePath == '' && not_file_upload_spe.checked == false)
                                                    {
                                                    alert('Please either select file to upload or check "No file to upload".');
                                                            return false;
                                                    }
                                                    }
                                                    else if (type == 'report')
                                                    {
                                                    var report_file = document.getElementById('report_file' + waveId).value;
                                                            var report_nofileupload = document.getElementById('report_nofileupload' + waveId);
                                                            if (material_name == '')
                                                    {
                                                    chk = 0;
                                                    }

                                                    if (report_file == '' && report_nofileupload.checked == false)
                                                    {
                                                    alert('Please either select file to upload or check "No file to upload".');
                                                            return false;
                                                    }
                                                    }
                                                    else if (type == 'fw_recap')
                                                    {
                                                    var recap_name = document.getElementById('recap_name' + waveId).value;
                                                            var planned_pos = document.getElementById('planned_pos' + waveId).value;
                                                            var planned_surveys = document.getElementById('planned_surveys' + waveId).value;
                                                            var actual_pos = document.getElementById('actual_pos' + waveId).value;
                                                            var actual_surveys = document.getElementById('actual_surveys' + waveId).value;
                                                            var actual_misfire = document.getElementById('actual_misfire' + waveId).value;
                                                            var actual_invalid = document.getElementById('actual_invalid' + waveId).value;
                                                            if (recap_name == '' || planned_pos == '' || planned_surveys == '' || actual_pos == '' || actual_surveys == '' || actual_misfire == '' || actual_invalid == '')
                                                    {
                                                    chk = 0;
                                                    }
                                                    }

                                                    if (chk == 0)
                                                    {
                                                    alert('Please fill all the required fields');
                                                            return false;
                                                    }
                                                    else
                                                            return true;
                                                            }

                                            function addScenario(type)
                                                    {
                                                    $('<div class="scenario-list-inner"><div class="wave-content-list"><label class="pull-left">Name:</label><input type="text" name="' + type + '_name[]" /></div><div class="wave-content-list"><label class="pull-left">Value:</label><input type="text" name="' + type + '_value[]" /></div><a href="javascript:;" style="float:left;" onclick="$(this).parent().remove();">x Delete this Scenario</a></div>').appendTo('.' + type + '-list-outer');
                                                            }
                                            function showHideEditWaveForm(id)
                                                    {
                                                    $('#' + id).slideToggle('slow');
                                                            }
                                            function addMoreFileLabel(waveId, counter, num)
                                                    {
                                                    $('<div class="wave-file-inner"><div class="wave-content-list-inner1"><b>Label:</b> &nbsp; <input type="text" name="label[' + num + '][]" /></div><div class="wave-content-list-inner1"><b>File:</b> &nbsp; <input type="file" name="path[' + num + '][]" /></div><div class="wave-content-list-inner2"><button class="btn btn-orange" onclick="$(this).parent().parent().remove();"><i class="icon-remove-sign"></i> &nbsp; Remove</button></div></div>').appendTo('#wave-file-outer' + counter + '-' + num + '-' + waveId);
                                                            }
                                            function addMoreFileMessage(waveId, counter, cnt)
                                                    {
                                                    var num = parseInt($('#wave-filemessage-outer' + counter + '-' + waveId).children('div.wave-filemessage-inner').length) + 1;
                                                            $('<div class="wave-filemessage-inner"><input type="hidden" name="formindex2[]" value="' + num + '" /><div class="wave-filemessage-heading">File & Message ' + num + '</div><div class="wave-filemessage-inner1"><div class="wave-content-list"><h5 class="page-header" style="font-size:20px;">Message</h5><textarea name="message[' + num + ']" style="width:98%; height:80px;"></textarea></div><div class="wave-content-list"><h5 class="page-header" style="font-size:20px;">File</h5><div id="wave-file-outer' + counter + '-' + num + '-' + waveId + '"><div class="wave-file-inner"><div class="wave-content-list-inner1"><b>Label:</b> &nbsp; <input type="text" name="label[' + num + '][]" /></div><div class="wave-content-list-inner1"><b>File:</b> &nbsp; <input type="file" name="path[' + num + '][]" /></div><div class="wave-content-list-inner2"><button class="btn btn-blue1" onclick="addMoreFileLabel(' + waveId + ',' + counter + ',' + num + '); return false;"><i class="icon-plus-sign"></i> &nbsp; Add more</button></div></div></div></div></div></div>').appendTo('#wave-filemessage-outer' + counter + '-' + waveId);
                                                            }
</script>
{% endblock %}